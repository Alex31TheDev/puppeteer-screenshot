{
    "name": "puppeteer-screenshot",
    "main": "index.js",
    "type": "module",
    "private": true,
    "scripts": {
        "format": "prettier --config .prettierrc --write **/*.{js,cjs,json}",
        "lint": "eslint . --config .eslintrc.json --ignore-path .gitignore --ext .js",
        "start": "node index.js",
        "minify-capture-tag": "terser \"./scripts/capture.js\" -o \"./scripts/capture.min.js\" -c passes=2 -m --mangle-props \"regex=/^#|_$/\" --toplevel "
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "express": "^4.21.2",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "pngjs": "^7.0.0",
        "puppeteer": "^24.26.1",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "re2": "^1.22.1",
        "winston": "^3.18.3"
    },
    "devDependencies": {
        "eslint": "^8.57.1",
        "prettier": "^3.4.2",
        "terser": "^5.44.0"
    }
}
